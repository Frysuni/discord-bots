{
  "license": "UNLICENSED",
  "main": "./dist/main",
  "scripts": {
    "boot": "npm i -g yarn && yarn install --prod && npm run build",
    "dev": "tsc-watch --inlineSourceMap --inlineSources --onSuccess \"node -r tsconfig-paths/register -r source-map-support/register ./dist/main.js\"",
    "build": "tsc --build --verbose && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "dependencies": {
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.1",
    "tscpaths": "^0.0.9"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.0.4"
  }
}
