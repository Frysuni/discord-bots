    "dev": "nest start --watch --debug 9229 --preserveWatchOutput",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./dist/ormconfig.js schema:sync"
  },
